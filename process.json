{
  "apps": [
    {
      "name": "starterkit-api-dev",
      "env": {
        "NODE_ENV": "dev",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/dev/starterkit-api.log"
    },
    {
      "name": "starterkit-api-sit",
      "env": {
        "NODE_ENV": "sit",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/sit/starterkit-api.log"
    },
    {
      "name": "starterkit-api-uat",
      "env": {
        "NODE_ENV": "uat",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/uat/starterkit-api.log"
    },
    {
      "name": "starterkit-api-uat-cluster",
      "env": {
        "NODE_ENV": "uat",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/uat/starterkit-api-cluster.log",
      "instances": 4,
      "exec_mode": "cluster"
    },
    {
      "name": "starterkit-api",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/prod/starterkit-api.log"
    },
    {
      "name": "starterkit-api-cluster",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "tip"
      },
      "script": "./bin/index.js",
      "watch": [
        "src"
      ],
      "ignore_watch": [
        "node_modules"
      ],
      "log_file": "./logs/prod/starterkit-api-cluster.log",
      "instances": 4,
      "exec_mode": "cluster"
    }
  ]
}
